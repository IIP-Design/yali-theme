{% extends 'base.twig' %}

{% block content %}
<section class="hero image_6-1" data-responsive-background-image>
     <img src="{{ header_url }}"
      sizes="{{ sizes }}"
      srcset="{{ srcset }}">
		<div class="hero-inner bottom_padd bottom_padd--thin">
    	<div class="hero-content ui container">
    		<h1 class="hero-h1">{{ page_data.title.rendered }}...</h1>    		
    	</div>
    </div>
</section>

<section class="ui container">	
	<div class='inner-container'>
		<section class='section yali-is_intro'>
			{{ page_data.content.rendered }}
		</section>		

		<section>{{ social_block }}</section>		
			
	</div>
</section>

{% endblock %}