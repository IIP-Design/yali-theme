<div class='cb-social full-width' style="background-color: {{ block_bg_color }}">
	<div class='ui container'>
		<h2 class='cb-social_title'>{{ title }}</h2>
		<a class='cb-social_icon' href='{{ facebook }}' target='_blank'>
			<i class="facebook f icon"></i>
		</a>
		<a class='cb-social_icon' href='{{ linkedin }}' target='_blank'>
			<i class="linkedin icon"></i>
		</a>
		<a class='cb-social_icon' href='{{ twitter }}' target='_blank'>
			<i class="twitter icon"></i>
		</a>
	</div>
</div>
